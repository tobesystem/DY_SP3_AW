<UserControl x:Class="dayouAWSWh2.UC.ucCellMonitor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:dayouAWSWh2.UC" 
             xmlns:Controls="http://metro.mahapps.com/winfx/xaml/controls" 
             xmlns:Icon="http://metro.mahapps.com/winfx/xaml/iconpacks"
             mc:Ignorable="d" 
             d:DesignWidth="1720" d:DesignHeight="950" Background="White" Unloaded="UserControl_Unloaded" Loaded="UserControl_Loaded">
    <Border Padding="8">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="40"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>

            <local:ucToolbarBtn x:Name="ucToolbarBtn"/>

            <Grid Grid.Row="1" Background="#FFE9ECEF">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="400"/>
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="0" Margin="0 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.45*"/>
                        <ColumnDefinition Width="0.55*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="40"/>
                    </Grid.RowDefinitions>

                    <Border Grid.Column="0" Grid.Row="0" Grid.RowSpan="5" Opacity="0.85" Background="#FFECECEC"
      BorderBrush="#FFCCCCCB" BorderThickness="3" CornerRadius="10" Margin="30">
                        <Border.Effect>
                            <DropShadowEffect Color="Black" BlurRadius="10" ShadowDepth="1"/>
                        </Border.Effect>
                        <TextBlock Text="SW" FontSize="60" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#FFADADAD"/>
                    </Border>

                    <Grid Grid.Column="1" Grid.Row="0" Margin="10 9 0 9" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="40"/>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <Border Grid.Column="0" Background="#FFCED4DA" Margin="0 0 1 0"/>
                            <Border Grid.Column="1" Background="#FFCED4DA" Margin="0 0 1 0"/>
                            <Border Grid.Column="2" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="19" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="3" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="18" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="4" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="17" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="5" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="16" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="6" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="15" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="7" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="14" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="8" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="13" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="9" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="12" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="10" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="11" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="11" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="10" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="12" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="09" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="13" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="08" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="14" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="07" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="15" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="06" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="16" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="05" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="17" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="04" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                            <Border Grid.Column="18" Background="#FFCED4DA" Margin="0 0 1 0">
                                <TextBlock Text="03" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                        <Border Grid.Column="19" Background="#FFCED4DA" Margin="0 0 1 0">
                            <TextBlock Text="02" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                        <Border Grid.Column="20" Background="#FFCED4DA" Margin="0 0 1 0">
                            <TextBlock Text="01" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </Grid>
                    <local:ucCellSp3 x:Name="ucCell05" Grid.Column="1" Grid.Row="4" Margin="10 0 0 10" />
                    <local:ucCellSp3 x:Name="ucCell06" Grid.Column="1" Grid.Row="3" Margin="10 0 0 10" />
                    <local:ucCellSp3 x:Name="ucCell07" Grid.Column="1" Grid.Row="2" Margin="10 0 0 10" />
                    <local:ucCellSp3 x:Name="ucCell08" Grid.Column="1" Grid.Row="1" Margin="10 0 0 10" />
                  


                    <!--범례-->
                    <Grid Width="990" Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="5" SnapsToDevicePixels ="True">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5">
                                <Border Width="18" Height="18" Background="White" BorderBrush="LightGray" BorderThickness="1"/>
                                <TextBlock Text="공셀" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5">
                                <Border Width="18" Height="18" Background="#FF66A80F" BorderBrush="LightGray" BorderThickness="1"/>
                                <TextBlock Text="파레트" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5">
                                <Border Width="18" Height="18" Background="#FF56DCFF" BorderBrush="LightGray" BorderThickness="1"/>
                                <TextBlock Text="제품(SP3)" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5">
                                <Border Width="18" Height="18" Background="#FF00D2B1" BorderBrush="LightGray" BorderThickness="1"/>
                                <TextBlock Text="제품(ETC)" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5">
                                <Border Width="18" Height="18" Background="#FFFFE066" BorderBrush="LightGray" BorderThickness="1"/>
                                <TextBlock Text="입고예약" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5">
                                <Border Width="18" Height="18" Background="#FFFF9800" BorderBrush="LightGray" BorderThickness="1"/>
                                <TextBlock Text="출고예약" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5">
                                <Border Width="18" Height="18" Background="RED" BorderBrush="LightGray" BorderThickness="1"/>
                                <TextBlock Text="이중입고" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5">
                                <Border Width="18" Height="18" Background="#FFBE4BDB" BorderBrush="LightGray" BorderThickness="1"/>
                                <TextBlock Text="공출고" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5">
                                <Border Width="18" Height="18" Background="Black" BorderBrush="LightGray" BorderThickness="1"/>
                                <TextBlock Text="금지셀" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="4,0,0,0"/>
                            </StackPanel>
                        </StackPanel>
                    </Grid>

                </Grid>

                <!--셀정보-->
                <Grid Grid.Column="1" Margin="25 20 25 5" SnapsToDevicePixels="True">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="100"/>
                        <RowDefinition Height="500"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <!--셀 상태-->
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="70"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="22"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="셀 상태" Grid.Row="0" FontSize="14" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Center" />
                        <TextBlock Text="입고위치" Grid.Row="1" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="셀 상태" Grid.Row="2" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                        <Border Grid.Column="1" Grid.Row="1" Background="White" BorderBrush="LightGray" BorderThickness="1" Margin="2 1">
                            <TextBlock x:Name="tbBank" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10,0,0,0"/>
                        </Border>
                        <Border Grid.Column="2" Grid.Row="1" Background="White" BorderBrush="LightGray" BorderThickness="1" Margin="2 1">
                            <TextBlock x:Name="tbBay" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10,0,0,0"/>
                        </Border>
                        <Border Grid.Column="3" Grid.Row="1" Background="White" BorderBrush="LightGray" BorderThickness="1" Margin="2 1">
                            <TextBlock x:Name="tbLevel" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10,0,0,0"/>
                        </Border>


                        <ComboBox x:Name="cbStatus" Grid.Column="1" Grid.ColumnSpan="3" Grid.Row="2" Style="{StaticResource CellCbStyle}" Height="28" FontSize="14"/>


                    </Grid>

                    <!--적재 정보-->
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="70"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="22"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="32"/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="적재 정보" Grid.Row="0" FontSize="14" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="적재코드" Grid.Row="1" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="제품코드1" Grid.Row="2" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="제품코드2" Grid.Row="3" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="제품코드3" Grid.Row="4" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="제품코드4" Grid.Row="5" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="제품코드5" Grid.Row="6" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="LOT no1" Grid.Row="7" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="LOT no2" Grid.Row="8" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="LOT no3" Grid.Row="9" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="LOT no4" Grid.Row="10" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="LOT no5" Grid.Row="11" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="파레트코드" Grid.Row="12" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <TextBlock Text="입고일시" Grid.Row="13" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center" />


                        <ComboBox x:Name="cbAlc"  Grid.Column="1" Grid.Row="1" Style="{StaticResource CellCbStyle}" FontSize="14"/>


                        <ComboBox x:Name="cbFrgubun" Grid.Column="2" Grid.Row="1" Style="{StaticResource CellCbStyle}" FontSize="14"/>

                        <Border Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="2" Background="White" BorderBrush="LightGray" BorderThickness="1" Margin="2 1">
                            <TextBlock x:Name="txtItemCode1" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0"/>
                        </Border>
                        <Border Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="3" Background="White" BorderBrush="LightGray" BorderThickness="1" Margin="2 1">
                            <TextBlock x:Name="txtItemCode2" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0"/>
                        </Border>
                        <Border Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="4" Background="White" BorderBrush="LightGray" BorderThickness="1" Margin="2 1">
                            <TextBlock x:Name="txtItemCode3" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0"/>
                        </Border>
                        <Border Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="5" Background="White" BorderBrush="LightGray" BorderThickness="1" Margin="2 1">
                            <TextBlock x:Name="txtItemCode4" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0"/>
                        </Border>
                        <Border Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="6" Background="White" BorderBrush="LightGray" BorderThickness="1" Margin="2 1">
                            <TextBlock x:Name="txtItemCode5" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0"/>
                        </Border>

                        <TextBox x:Name="tbLotno1" FontSize="14" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="7" BorderBrush="LightGray" Margin="2 1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="24"/>
                        <TextBox x:Name="tbLotno2" FontSize="14" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="8" BorderBrush="LightGray" Margin="2 1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="24"/>
                        <TextBox x:Name="tbLotno3" FontSize="14" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="9" BorderBrush="LightGray" Margin="2 1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="24"/>
                        <TextBox x:Name="tbLotno4" FontSize="14" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="10" BorderBrush="LightGray" Margin="2 1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="24"/>
                        <TextBox x:Name="tbLotno5" FontSize="14" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="11" BorderBrush="LightGray" Margin="2 1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="24"/>
                        <TextBox x:Name="tbPltcode" FontSize="14" Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="12" BorderBrush="LightGray" Margin="2 1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" MaxLength="6"/>

                        <DatePicker x:Name="dtIndate" Grid.Column="1" Grid.Row="13" 
                                    Background="White" BorderBrush="LightGray" Margin="2 1" FontSize="14"
                                    VerticalContentAlignment="Center" Focusable = "False" SelectedDateFormat="Short" 
                                     Padding="10 3 5 0"
                                    CalendarStyle="{StaticResource CustomCalendarStyle }" Loaded="dtIndate_Loaded" />

                        <Border Grid.Column="2" Grid.Row="13" Margin="2 1" Background="White" BorderBrush="LightGray" BorderThickness="1">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="40"/>
                                </Grid.ColumnDefinitions>
                                <TextBox x:Name="tbTime" Text="" PreviewMouseDown="TimeTextBox_PreviewMouseDown" Style="{StaticResource textBoxStyle}" BorderThickness="0"
                                     PreviewTextInput="timeTextBox_PreviewTextInput" LostFocus="timeTextBox_LostFocus" FontSize="14"
                                     Margin="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Padding="10 0 0 0"/>
                                <StackPanel Grid.Column="1" Background="White">
                                    <Button Click="IncreaseTimeButton_Click" Background="White" BorderBrush="{x:Null}">
                                        <Image Source="/Images/위화살표검.png" Height="10" RenderOptions.BitmapScalingMode="HighQuality"/>
                                    </Button>
                                    <Button Click="DecreaseTimeButton_Click" Background="White" BorderBrush="{x:Null}">
                                        <Image Source="/Images/아래화살표검.png" Width="10" RenderOptions.BitmapScalingMode="HighQuality" />
                                    </Button>
                                </StackPanel>
                            </Grid>
                        </Border>
                    </Grid>

                    <!--셀 사용현황-->
                    <Grid Grid.Row="2" Margin="0 0 0 0" VerticalAlignment="Bottom">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="22"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="셀 사용현황" Grid.Row="0" FontSize="14" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Top" />
                        <DataGrid x:Name="dgCellInfo" Grid.Row="1" Style="{DynamicResource MonitorDataGrid}" CellStyle="{DynamicResource CellDataGrid}" ColumnHeaderStyle="{DynamicResource MonitorDataGridHeader}" RowStyle="{DynamicResource MonitorDataGridRow}" Margin="-8 0 -14 -4" Cursor="Arrow" LoadingRow="dgCellInfo_LoadingRow">
                            <DataGrid.Columns>
                                <DataGridTextColumn Header="열" IsReadOnly="True" Width="52" Binding="{Binding IDBANK}" ElementStyle="{StaticResource MonitorDataGridCellCenter}"/>
                                <DataGridTextColumn Header="사용률" IsReadOnly="True" Width="52" Binding="{Binding CELL_PERCENT}" ElementStyle="{StaticResource MonitorDataGridCellCenter}"/>
                                <DataGridTextColumn Header="총셀" IsReadOnly="True" Width="50" Binding="{Binding CELL_TOT}" ElementStyle="{StaticResource MonitorDataGridCellCenter}"/>
                                <DataGridTextColumn Header="실셀" IsReadOnly="True" Width="50" Binding="{Binding CELL_CNT1}" ElementStyle="{StaticResource MonitorDataGridCellCenter}"/>
                                <DataGridTextColumn Header="공셀" IsReadOnly="True" Width="50" Binding="{Binding CELL_CNT2}" ElementStyle="{StaticResource MonitorDataGridCellCenter}"/>
                                <DataGridTextColumn Header="금지셀" IsReadOnly="True" Width="52" Binding="{Binding CELL_CNT3}" ElementStyle="{StaticResource MonitorDataGridCellCenter}"/>
                                <DataGridTextColumn Header="입고예약" IsReadOnly="True" Width="60" Binding="{Binding CELL_CNT4}" ElementStyle="{StaticResource MonitorDataGridCellCenter}"/>
                                <DataGridTextColumn Header="출고예약" IsReadOnly="True" Width="60" Binding="{Binding CELL_CNT5}" ElementStyle="{StaticResource MonitorDataGridCellCenter}"/>

                            </DataGrid.Columns>
                        </DataGrid>
                    </Grid>


                </Grid>


            </Grid>

        </Grid>
    </Border>
</UserControl>
