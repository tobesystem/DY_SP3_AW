<UserControl x:Class="dayouAWSWh2.UC.ucSystemSettingTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:dayouAWSWh2.UC"
             xmlns:Icon="http://metro.mahapps.com/winfx/xaml/iconpacks"
             mc:Ignorable="d" 
             d:DesignHeight="995" d:DesignWidth="1720">
    <Grid>
        <TabControl>
            <TabItem Header="SW 시스템설정">
                <local:ucSystemSetting x:Name="ucSystemSetting" Grid.Row="0" />
            </TabItem>

            <TabItem Header="SP3 시스템설정">
                <local:ucSystemSettingSP3 x:Name="ucSystemSettingSP3" Grid.Row="0" Grid.ColumnSpan="2"/>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
