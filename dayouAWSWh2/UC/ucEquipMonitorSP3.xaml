                                </Border>


                                <TextBlock Text="LOT no1" Grid.Column="0" Grid.Row="5" FontSize="12" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                                <TextBox x:Name="tbLotno1" FontSize="12" Grid.Column="1" Grid.Row="5" BorderBrush="LightGray" Margin="1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="24"/>


                                <TextBlock Text="LOT no2" Grid.Column="0" Grid.Row="6" FontSize="12" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                                <TextBox x:Name="tbLotno2" FontSize="12" Grid.Column="1" Grid.Row="6" BorderBrush="LightGray" Margin="1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="24"/>

                                <TextBlock Text="LOT no3" Grid.Column="0" Grid.Row="7" FontSize="12" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                                <TextBox x:Name="tbLotno3" FontSize="12" Grid.Column="1" Grid.Row="7" BorderBrush="LightGray" Margin="1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="24"/>


                                <TextBlock Text="LOT no4" Grid.Column="0" Grid.Row="8" FontSize="12" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                                <TextBox x:Name="tbLotno4" FontSize="12" Grid.Column="1" Grid.Row="8" BorderBrush="LightGray" Margin="1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="24"/>


                                <TextBlock Text="LOT no5" Grid.Column="0" Grid.Row="9" FontSize="12" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                                <TextBox x:Name="tbLotno5" FontSize="12" Grid.Column="1" Grid.Row="9" BorderBrush="LightGray" Margin="1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="24"/>

                                <TextBlock Text="파레트코드" Grid.Column="0" Grid.Row="10" FontSize="12" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                                <TextBox x:Name="tbPlt" FontSize="12" Grid.Column="1" Grid.Row="10" BorderBrush="LightGray" Margin="1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" MaxLength="6"/>

                                <TextBlock Text="COMMIT NO" Grid.Column="0" Grid.Row="11" FontSize="12" Foreground="DimGray" HorizontalAlignment="Left" VerticalAlignment="Center"/>

                                <TextBox x:Name="tbCommit" FontSize="12" Grid.Column="1" Grid.Row="11" BorderBrush="LightGray" Margin="1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" MaxLength="4"/>

