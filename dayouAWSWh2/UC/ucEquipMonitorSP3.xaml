                                <TextBox x:Name="tbBody" FontSize="12" Grid.Column="1" Grid.Row="12" BorderBrush="LightGray" Margin="1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" MaxLength="10"/>

                                <Grid Grid.Row="13" Grid.ColumnSpan="2">
                                    <Button x:Name="btnUpdate" Content="수정" Width="45" Margin="1" Style="{StaticResource btnSet}" HorizontalAlignment="Right" Click="btnUpdate_Click"/>
                                </Grid>
                            </Grid>
                        </Grid>

                        <!--CV 데이터-->
                        <Grid Grid.Row="4">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="24"/>
                                <RowDefinition Height="24"/>
                            </Grid.RowDefinitions>
                            <TextBlock Text="CV 데이터" Grid.Row="0" FontSize="12" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="50"/>
                                    <ColumnDefinition Width="65"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Border Grid.Column="0" Grid.Row="0" Background="White" BorderBrush="LightGray" BorderThickness="1" Margin="2">
                                    <TextBlock x:Name="txtBuff" FontSize="12" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0"/>
                                </Border>

                                <TextBox x:Name="txtBuffTarget" Grid.Column="1" Grid.Row="3" Height="20" VerticalContentAlignment="Center" 
                           HorizontalContentAlignment="Center" PreviewTextInput="txtBuffTarget_PreviewTextInput" 
                           PreviewKeyDown="txtBuffTarget_PreviewKeyDown" MaxLength="3" 
                           IsReadOnly="False" PreviewMouseLeftButtonDown="txtBuffTarget_PreviewMouseLeftButtonDown"/>

                                <StackPanel Grid.Column="2" Orientation="Horizontal" HorizontalAlignment="Right">
                                    <Button x:Name="btnCvcMove" Content="이동" Width="45" Margin="1" Style="{StaticResource btnSet}" Click="btnCvcMove_Click"/>
                                    <Button x:Name="btnCvcDel" Content="삭제" Width="45" Margin="1" Style="{StaticResource btnSet}" Click="btnCvcDel_Click"/>
                                    <Button x:Name="btnCvcSwap" Content="교환" Width="45" Margin="1" Style="{StaticResource btnSet}" Click="btnCvcSwap_Click"/>
                                </StackPanel>


                            </Grid>
                        </Grid>

                        <!--CV 지시-->
                        <Grid Grid.Row="5">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="24"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <TextBlock Text="CV 지시" Grid.Row="0" FontSize="12" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="24"/>
                                    <RowDefinition Height="24"/>
                                </Grid.RowDefinitions>

                                <Button x:Name="btnCvGo" Grid.Column="0" Grid.Row="0" Content="직진지시" Margin="1" Style="{StaticResource btnSet}" Click="btnCvGo_Click"/>
                                <Button x:Name="btnCvGoComp" Grid.Column="0" Grid.Row="1" Content="직진완료" Margin="1" Style="{StaticResource btnSet}" Click="btnCvGoComp_Click"/>
                                <Button x:Name="btnCvQua" Grid.Column="1" Grid.Row="0" Content="분기지시" Margin="1" Style="{StaticResource btnSet}" Click="btnCvQua_Click"/>
                                <Button x:Name="btnCvQuaComp" Grid.Column="1" Grid.Row="1" Content="분기완료" Margin="1" Style="{StaticResource btnSet}" Click="btnCvQuaComp_Click"/>
                                <Button x:Name="btnCvJoin" Grid.Column="2" Grid.Row="0" Content="합류지시" Margin="1" Style="{StaticResource btnSet}" Click="btnCvJoin_Click"/>
                                <Button x:Name="btnCvJoinComp" Grid.Column="2" Grid.Row="1" Content="합류완료" Margin="1" Style="{StaticResource btnSet}" Click="btnCvJoinComp_Click"/>

                            </Grid>
                        </Grid>

                    </Grid>
                </Border>

                <!--바코드 리딩정보, 의장/도장 수신정보-->
                <Border Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="1"  BorderBrush="#FFC1C2C3" BorderThickness="0" Margin="0 5 -2 0">
                    <Grid >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.26*"/>
                            <ColumnDefinition Width="0.34*"/>
                            <ColumnDefinition Width="0.4*"/>
                        </Grid.ColumnDefinitions>

                        <!--바코드 리딩정보-->
                        <Border Grid.Column="0" Margin="10 0 0 0" Background="#FFF5F6F7" BorderBrush="LightGray" BorderThickness="1" CornerRadius="5 5 0 0" SnapsToDevicePixels="True">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="20"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <Border Style="{StaticResource equipBd}" Width="130" Height="20" Margin="10 -2 0 0" BorderThickness="1 0 1 1" CornerRadius="0 0 3 3" Background="#FFBB4055" HorizontalAlignment="Left" VerticalAlignment="Top" >
                                    <TextBlock Text="바코드 리딩 정보" FontSize="14" Height="14" Foreground="White" HorizontalAlignment="Center" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Center" />
                                </Border>

                                <Border Grid.Row="1" Background="#FFE1E4E8" BorderBrush="LightGray" BorderThickness="1" Margin="5 1" >
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="77"/>
                                            <ColumnDefinition Width="1"/>
                                            <ColumnDefinition Width="0.32*"/>
                                            <ColumnDefinition Width="1"/>
                                            <ColumnDefinition Width="0.32*"/>
                                            <ColumnDefinition Width="1"/>
                                            <ColumnDefinition Width="0.16*"/>
                                            <ColumnDefinition Width="0.2*"/>
                                        </Grid.ColumnDefinitions>

                                        <!-- 경계선 -->
                                        <Border Grid.Column="1" Background="White" Margin="0 2" />
                                        <Border Grid.Column="3" Background="White" Margin="0 2" />
                                        <Border Grid.Column="5" Background="White" Margin="0 2" />

                                        <Border Grid.Column="0" Background="#FFE1E4E8" >
                                            <TextBlock Text="[입고] 561" FontSize="14" Foreground="DimGray" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"/>
                                        </Border>

                                        <Border x:Name="bdBcrIn" Grid.Column="2"  Margin="0" Tag="[입고] 561" Background="Transparent" MouseLeftButtonDown="bdBcrIn_MouseLeftButtonDown">
                                            <TextBlock x:Name="txtPltInCodeBcr" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>

                                        <Border Grid.Column="4" >
                                            <TextBlock x:Name="txtPltInCode" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>


                                        <Border x:Name="bdBcrInOk" Grid.Column="6" >
                                            <TextBlock x:Name="txtBcrInOk" FontSize="14" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>

                                        <Button x:Name="btnReBcrIn" Content="재시도" FontSize="14" Grid.Column="7" Style="{StaticResource btnSet}" Margin="-1" Click="btnReBcr_Click"/>
                                    </Grid>
                                </Border>
                                <Border Grid.Row="2" Background="#FFE1E4E8" BorderBrush="LightGray" BorderThickness="1" Margin="5 1" >
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="77"/>
                                            <ColumnDefinition Width="1"/>
                                            <ColumnDefinition Width="0.32*"/>
                                            <ColumnDefinition Width="1"/>
                                            <ColumnDefinition Width="0.32*"/>
                                            <ColumnDefinition Width="1"/>
                                            <ColumnDefinition Width="0.16*"/>
                                            <ColumnDefinition Width="0.2*"/>
                                        </Grid.ColumnDefinitions>

                                        <!-- 경계선 -->
                                        <Border Grid.Column="1" Background="White" Margin="0 2" />
                                        <Border Grid.Column="3" Background="White" Margin="0 2" />
                                        <Border Grid.Column="5" Background="White" Margin="0 2" />

                                        <Border Grid.Column="0" Background="#FFE1E4E8" >
                                            <TextBlock Text="[출고] 713" FontSize="14" Foreground="DimGray" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"/>
                                        </Border>

                                        <Border x:Name="bdBcrOut" Grid.Column="2" Margin="0" Tag="[출고] 713" Background="Transparent" MouseLeftButtonDown="bdBcrOut_MouseLeftButtonDown">
                                            <TextBlock x:Name="txtPltOutCodeBcr" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>

                                        <Border Grid.Column="4">
                                            <TextBlock x:Name="txtPltOutCode" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>

                                        <Border x:Name="bdBcrOutOk" Grid.Column="6" >
                                            <TextBlock x:Name="txtBcrOutOk" Foreground="White" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>

                                        <Button x:Name="btnReBcrOut" Content="재시도" FontSize="14" Grid.Column="7" Style="{StaticResource btnSet}" Margin="-1" Click="btnReBcrOut_Click"/>
                                    </Grid>
                                </Border>

                                <StackPanel x:Name="stpBcrInput" Grid.Row="0" Orientation="Horizontal" Width="220" Margin="0 0 0 0" HorizontalAlignment="Right">
                                    <TextBlock x:Name="txtBcrName" Text="" FontSize="12" Foreground="Red" FontWeight="Bold" Width="70" Margin="0 2" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"/>
                                    <TextBox x:Name="tbBcrPlt" Width="90" FontSize="12" Margin="1" Padding="10 0 0 0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"  MaxLength="6"/>
                                    <Button x:Name="btnBcrIn" Content="수동입력" Width="55" FontSize="12" Grid.Column="4" Margin="0" Click="btnBcrIn_Click" Style="{StaticResource btnSet}"/>
                                </StackPanel>

                            </Grid>
                        </Border>

                        <Grid Grid.Column="1" Margin="10 0" SnapsToDevicePixels="True">
                            <!--의장 수신 정보-->
                            <Border Background="#FFF5F6F7" BorderBrush="LightGray" BorderThickness="1" CornerRadius="5 5 0 0" Margin="0 0 0 0">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="20"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>


                                    <Border Style="{StaticResource equipBd}" Width="160" Height="20" Margin="10 -2 0 0" BorderThickness="1 0 1 1" CornerRadius="0 0 3 3" Background="#FFBB4055" HorizontalAlignment="Left" VerticalAlignment="Top" >
                                        <TextBlock Text="의장 수신 정보" FontSize="14" Height="14" Foreground="White" HorizontalAlignment="Center" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Center" />
                                    </Border>

                                    <Border Grid.Column="0" Grid.Row="1" Background="#FFE1E4E8" BorderBrush="LightGray" BorderThickness="1"  Margin="5 2">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="40"/>
                                                <ColumnDefinition Width="1"/>
                                                <ColumnDefinition Width="60"/>
                                                <ColumnDefinition Width="1"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*"/>
                                                <RowDefinition Height="1"/>
                                                <RowDefinition Height="*"/>
                                            </Grid.RowDefinitions>
                                            <!-- 경계선 -->
                                            <Border Grid.Column="1" Grid.Row="0" Grid.RowSpan="3" Background="White" Margin="0 5"/>
                                            <Border Grid.Column="1" Grid.ColumnSpan="4" Grid.Row="1" Background="White" Margin="0 0 5 0"/>
                                            <Border Grid.Column="3" Grid.Row="0" Grid.RowSpan="3" Background="White" Margin="0 5"/>

                                            <Border Grid.Column="0" Grid.Row="0" Grid.RowSpan="3" Background="#FFE1E4E8" >
                                                <TextBlock Text="의장" FontSize="14" Foreground="DimGray" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            </Border>

                                            <Border Grid.Column="2" Grid.Row="0">
                                                <TextBlock Text="최종 수신" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            </Border>

                                            <Border Grid.Column="4" Grid.Row="0">
                                                <TextBlock x:Name="txtHostDate" Text="" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            </Border>

                                            <Border Grid.Column="2" Grid.Row="2">
                                                <TextBlock Text="수신 상태" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            </Border>
                                            <Border Grid.Column="4" Grid.Row="2">
                                                <TextBlock x:Name="txtHostOption" Text="" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                            </Border>
                                        </Grid>
                                    </Border>

                                </Grid>
                            </Border>

                        </Grid>

                        <Border Grid.Column="2" Background="#FFE9ECEF" Margin="100 0 10 0" BorderBrush="LightGray" BorderThickness="1" CornerRadius="5 5 0 0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="23"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>

                                <Border Style="{StaticResource equipBd}" Width="160" Height="22" Margin="10 -1 0 0" BorderThickness="1 0 1 1" CornerRadius="0 0 3 3" Background="#FFBB4055" HorizontalAlignment="Left" VerticalAlignment="Top" >
                                    <TextBlock Text="컨베이어 에러정보" FontSize="14" Height="14" Foreground="White" HorizontalAlignment="Center" FontWeight="Bold" TextWrapping="Wrap" VerticalAlignment="Center" />
                                </Border>

                                <ListBox x:Name="lbCvError" Grid.Row="1" Margin="4">
                                    <ListBox.ItemTemplate>
                                        <DataTemplate>
                                            <TextBlock Text="{Binding ERR_MSG}" />
                                        </DataTemplate>
                                    </ListBox.ItemTemplate>
                                </ListBox>
                            </Grid>
                        </Border>



                    </Grid>
                </Border>

            </Grid>

       
        </Grid>
    </Border>
</UserControl>
